@model ClassAnalytics.Models.Uploads_Models.UploadModel
@{
    ViewBag.Title = "Viewing " + Model.uploadName;
    string newLink = "http://localhost:5753/" + Model.relativePath;
}

<center><h2>@ViewBag.Title</h2></center>
<h5 style="text-align:right;margin-top:-30px;">@Html.ActionLink("Return to Dashboard", "Index", "Class")</h5>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10" style="margin: 0 auto;">
        <object data="@newLink" height="500" width="100%" type="application/pdf">
            <b>This browser does not support pdf files.</b>
        </object>
    </div>
    <div class="col-md-1"></div>
</div>